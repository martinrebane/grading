package ee.ttu.kert.maria.script;

import java.io.IOException;

public class GistCreator {
	
	private String link;
	
	public void run() {
		try {
			Process process = Runtime.getRuntime().exec("ruby ../../../../../../../../../bash/gistcreator.rb");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

	public String getLink() {
		return link;
	}
}
